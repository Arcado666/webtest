<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.suren.autotest.platform.mapping.AttachConfigMapper">
	<insert id="save">
		<selectKey keyProperty="id" resultType="String" order="BEFORE">
			SELECT UUID()
		</selectKey>
		
		insert into attach_config (id, name, remark) values (
			#{id}, #{name}, #{remark}
		)
	</insert>
	
	<delete id="delById">
		delete from attach_config where id = #{id}
	</delete>
	
	<select id="getById" resultType="AttachConfig">
		select * from attach_config where id = #{id}
	</select>
	
	<select id="getAll" resultType="AttachConfig">
		select * from attach_config order by name
	</select>
	
	<resultMap type="org.suren.autotest.platform.model.AttachConfig" id="attachConfigMap">
	</resultMap>
</mapper>